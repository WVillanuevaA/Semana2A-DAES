<Window x:Class="Semana2A.Mantenimiento"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Semana2A"
        mc:Ignorable="d"
        Title="Mantenimiento" Height="450" Width="800">
    <Grid>
        <Menu>
            <MenuItem x:Name="menu_opciones" Header="OPCIONES">
                <MenuItem x:Name="menu_registro" Header="Registro" Click="menu_registro_Click"></MenuItem>
                <MenuItem x:Name="menu_lista" Header="Lista" Click="menu_lista_Click"></MenuItem>
            </MenuItem>
        </Menu>
    </Grid>
</Window>
